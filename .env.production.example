# Environment Configuration
ENVIRONMENT=production

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Logging Configuration
RUST_LOG=info

# Executor Configuration
# IMPORTANT: Set your actual private key here
EXECUTOR_PRIVATE_KEY=your-private-key-here
SHARD_NUMBER=1
SUBMIT_BATCH_SIZE=10

# Production RPC Configuration (REQUIRED)
BASE_RPC_URL='rpc_url'

# Auto-prove transactions (set to false if you want manual control)
AUTO_PROVE=true